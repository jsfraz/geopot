{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "vite-dev",
      "type": "shell",
      "isBackground": true,
      "command": "cd ${workspaceFolder}/frontend && npm run dev",
      "presentation": {
        "reveal": "always",
        "panel": "dedicated"
      },
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": ".",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": "VITE v\\d+\\.\\d+\\.\\d+  ready in \\d+"
          }
        }
      ]
    }
  ]
}
